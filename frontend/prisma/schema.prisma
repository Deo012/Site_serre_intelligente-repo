generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Connections {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Users {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @db.Date
  password  String
  username  String
}
